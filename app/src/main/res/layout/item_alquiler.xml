<androidx.cardview.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    card_view:cardCornerRadius="4dp"
    card_view:cardElevation="2dp"
    card_view:cardUseCompatPadding="true"
    card_view:contentPadding="5dp"
    android:layout_margin="10dp"
    tools:ignore="NamespaceTypo">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/idConstrUnoAlq"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            card_view:layout_constraintHorizontal_bias="0.0">

            <ImageView
                android:id="@+id/ivFhIni"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                card_view:layout_constraintStart_toStartOf="parent"
                card_view:layout_constraintTop_toTopOf="parent"
                card_view:srcCompat="@drawable/login_black_24dp" />

           <!-- <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="24dp"
                android:layout_marginTop="2dp"
                android:text="Entrada: "
                card_view:layout_constraintStart_toStartOf="parent"
                card_view:layout_constraintTop_toTopOf="parent" />-->


            <TextView
                android:id="@+id/tvFhInicio"
                android:layout_width="wrap_content"
                android:layout_height="24dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="4dp"
                android:text="fh Entrada..."
                app:layout_constraintTop_toTopOf="parent"
                card_view:layout_constraintStart_toEndOf="@+id/ivFhIni" />

            <!-- <TextView
                android:id="@+id/tvfhc"
                android:layout_width="wrap_content"
                android:layout_height="24dp"
                android:layout_marginTop="4dp"
                android:text="Salida:"
                card_view:layout_constraintStart_toStartOf="parent"
                card_view:layout_constraintTop_toBottomOf="@+id/ivFhIni" />-->

            <ImageView
                android:id="@+id/ivFhFin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"


                android:layout_marginTop="4dp"
                android:layout_marginEnd="128dp"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintTop_toTopOf="parent"
                card_view:srcCompat="@drawable/logout_black_24dp" />

            <TextView
                android:id="@+id/tvFhFin"
                android:layout_width="wrap_content"
                android:layout_height="24dp"
                android:layout_marginTop="4dp"
                android:layout_marginStart="10dp"
                android:text="fh Salida"
                card_view:layout_constraintStart_toEndOf="@+id/ivFhFin"
                card_view:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/ivInmueble"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                card_view:srcCompat="@drawable/home_black_24dp"
                card_view:layout_constraintStart_toStartOf="parent"
                card_view:layout_constraintTop_toBottomOf="@+id/ivFhIni"/>

          <!--  <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:text="Inmueble: "
                card_view:layout_constraintStart_toStartOf="parent"
                card_view:layout_constraintTop_toBottomOf="@+id/tvfhc" />-->

            <!--    <TextView
                android:id="@+id/tvnombrec"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="52dp"
                android:layout_marginTop="5dp"
                android:text="Nombre:"
                card_view:layout_constraintStart_toStartOf="parent"
                card_view:layout_constraintTop_toBottomOf="@+id/ivInmueble" />-->

            <TextView
                android:id="@+id/tvnombreInmueble"
                android:layout_width="wrap_content"
                android:layout_height="24dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="4dp"
                android:text="nombre Inmueble"
                card_view:layout_constraintStart_toEndOf="@+id/ivInmueble"
                card_view:layout_constraintTop_toBottomOf="@+id/tvFhInicio" />

            <ImageView
                android:id="@+id/ivCliente"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                card_view:layout_constraintStart_toStartOf="parent"
                card_view:layout_constraintTop_toBottomOf="@+id/ivInmueble"
                card_view:srcCompat="@drawable/person_black_24dp" />

            <TextView
                android:id="@+id/tvnombreCliente"
                android:layout_width="wrap_content"
                android:layout_height="24dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="4dp"
                android:text="nombre Cliente"
                card_view:layout_constraintStart_toEndOf="@+id/ivCliente"
                card_view:layout_constraintTop_toBottomOf="@+id/tvnombreInmueble" />

            <ImageView
                android:id="@+id/ivPortal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:layout_marginEnd="128dp"
                card_view:layout_constraintEnd_toEndOf="parent"
                card_view:layout_constraintTop_toBottomOf="@+id/ivFhFin"
                card_view:srcCompat="@drawable/web_black_24dp" />

            <TextView
                android:id="@+id/tvNombrePortal"
                android:layout_width="wrap_content"
                android:layout_height="24dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="4dp"
                android:text="nombre Portal"
                card_view:layout_constraintStart_toEndOf="@+id/ivPortal"
                card_view:layout_constraintTop_toBottomOf="@+id/tvFhFin" />

        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>
